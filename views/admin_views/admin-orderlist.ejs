            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">


                            <!-- <input type="text" placeholder="Search..." class="form-control"> -->


                        </div>


                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="order">

                            <!-- <div>
                                <input type="text" id="searchInput" placeholder="Search...">
                                <button id="searchButton">Search</button>
                            </div> -->

                            <thead>
                                <tr>
                                    <th>Order_ID</th>
                                    <th scope="col">User name</th>
                                    <!-- <th scope="col">Email</th> -->
                                    <th scope="col">Total Amount</th>
                                   
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-center"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if (!orderDetails || orderDetails.length === 0) { %>
                                    <h6>No orders</h6>
                                <% } else { %>
                                    <% orderDetails.reverse().forEach((order) => { %>
                                        
                                <tr>
                                    <td><%= 'OID100' + order._id.toString().substring(21,24) %></td>
                                   
                                    
                                    <td><b><%= order.userId?.username %></b></td>
                                    
                                    <td>₹<%= order.totalAmount %></td>
                                    
                                    <td><%= order.createdAt.toLocaleDateString()%></td>
                                    <td class="text-center">
                                        <a href="/admin/order-manage/<%= order._id %>" class="btn btn-sm rounded font-sm">View Details</a>
                                        
                                    </td>
                                </tr>
                                    
                                    <% }); %>
                                <% } %>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->

           
            <!-- pagination-area -->
        <% if(orderDetails.length > 0) {%>
            <div class="pagination-area mt-30 mb-50">
                <nav aria-label="Page navigation ">
                    <ul class="pagination justify-content-start">
                        <% if(current ==1){ %>
                            <li class="page-item disabled"><a href="#" class="page-link">First</a></li>
                            <% }else{ %>
                            <li class="page-item"><a href="/admin/order-list/?page=1" class="page-link">First</a></li>
                            <% } %>
    
                        <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                        <% if (i !== 1) { %>
                            <li class="page-item "><a href="/admin/order-list/?page=<%= i %>" class="page-link">...</a></li>
                        <% } %>
     
                        <% for(; i<= (Number(current) + 4) && i < pages; i++){ %>
                        <% if (i == current) { %>
                            <li class="page-item disabled"><a href="#" class="page-link"><%= i %></a></li>
                        <% }else{ %>
                            <li class="page-item"><a href="/admin/order-list/?page=<%= i %>" class="page-link"><%= i %></a></li>
                            <% } %>   
                        <% if (i == Number(current) + 4 && i < pages) { %>
                            <li class="page-item"><a href="/admin/order-list/?page=<%= i %>" class="page-link">...</a></li>
                            <% } %>
                            <% } %>
       
                        <% if (current == pages) { %>
                            <li class="page-item disabled"><a href="#" class="page-link" style="border-radius: 0.25rem">End</a></li> 
                            <% }else{ %>
                            <li class="page-item"><a href="/admin/order-list/?page=<%= pages %>" class="page-link material-icons md-chevron_right" style="border-radius: 0.25rem">End</a></li>
                            <% } %>
                    </ul>
                </nav>
            </div>
            <% } %>

  

        </section> <!-- content-main end// -->
